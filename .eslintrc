{
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "rules": {
        "no-continue": 0,
        "vars-on-top": 0,
        "one-var": 0,
        "no-var": 0,
        "import/order": 0,
        "func-names": 0,
        "no-console": 0,
        "prefer-const": 0,
        "no-restricted-syntax": 0,
        "no-plusplus": 0,
        "no-use-before-define": 0,
        "quote-props": 0,
        "dot-notation": 0,
        "prefer-template": 1,
        "prefer-destructuring": 0,
        "max-len": 0,
        "no-tabs": 0,
        "no-param-reassign": 0,
        "radix": 0,
        "consistent-return": 0,
        "no-else-return": 0,
        "no-restricted-properties": 0,
        "no-restricted-globals": 0,
        "object-shorthand": 0,
        "indent": 0,
        "eqeqeq": 0,
        "no-multiple-empty-lines": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "mocha": true,
        "browser": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint"
}